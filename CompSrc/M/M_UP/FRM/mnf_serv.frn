.create view loSpOrder
 var cSopr : comp;
 from SpOrder
    , KatMc
    , KatEd
 where((cSopr   == SpOrder.cSpSopr
   and  word(1) == SpOrder.VidOrder
   and  SpOrder.cMc == KatMc.nRec
   and  KatMc.cEd == KatEd.nRec
      ));
.fields
 CommonFormHeader
N_TTN F_KUDA
field_DATE
N_DOC_BASE field_Ot_DATE
field_FROM NAME_FROM
From_FIO_MOL
field_TO NAME_TO
field_MOL to_FIO_Mol
field_KOGO
if( IsValidAll(tnKatMc)
  , loSpOrder.KatMc.Name
  , '' )
if( IsValidAll(tnKatMc)
  , loSpOrder.KatEd.Name
  , '' )
if( IsValidAll(tnSpOrder)
  , Trim(String(Round(loSpOrder.SpOrder.Kol,ForMnf_Kol)))
  , '' )
if( IsValidAll(tnSpOrder)
  , Trim(String(Round(loSpOrder.SpOrder.SrPrice,ForMnf_Price)))
  , '')
if( IsValidAll(tnSpOrder)
  , Trim(String(Round( loSpOrder.SpOrder.Kol
                       * loSpOrder.SpOrder.SrPrice
                     , ForMnf_Price ) ) )
  , ''
  )
OSUMM

OSUMM
OVSUMM
.endfields
