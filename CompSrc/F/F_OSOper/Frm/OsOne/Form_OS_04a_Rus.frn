.{
.begin
  СчетчикДрагметалов := 0;
  СчСуДб := СчетДеб + ' ' + if( trim(СубсчетДеб)='00','',СубсчетДеб)
  СчСуКр := СчетКре + ' ' + if( trim(СубсчетКре)='00','',СубсчетКре)
  OborotNpp := 0
end.
#CommissionLinkInit(osOne)
.fields
  '(Метод учета: ' +Метод_Учета +')'
