.begin
  if (Доверенность_номер <> '') and (Longint(Дата_документа) <> 0) and (Доверенное_лицо_ФИО <> '')
    Задана_Доверенность := true;
  else
    Задана_Доверенность := false;
end.
.fields
  // Сдал
  МОЛ_Должность_Поставщика
  МОЛ_Поставщика

  // Принял
  if(Доверенное_лицо_ФИО = '', МОЛ_Должность_Получателя, Доверенное_лицо_должность)
  if(Доверенное_лицо_ФИО = '', МОЛ_Получателя, Доверенное_лицо_ФИО)

  // Сдал
  Табельный_номер_Поставщика

  // Дата под графой "Принял"
  if((Доверенное_лицо_ФИО = '') or (Longint(Накладная_дата) = 0), '', Day(Накладная_дата))
  if((Доверенное_лицо_ФИО = '') or (Longint(Накладная_дата) = 0), '', DateToStr(Накладная_дата, 'mon'))
  if((Доверенное_лицо_ФИО = '') or (Longint(Накладная_дата) = 0), '20____', Year(Накладная_дата))

  // Доверенность
  if(Задана_Доверенность, Day(Доверенность_дата_выдачи), '')
  if(Задана_Доверенность, DateToStr(Доверенность_дата_выдачи, 'mon'), '')
  if(Задана_Доверенность, DateToStr(Доверенность_дата_выдачи, 'YYYY'), '20____')
  if(Задана_Доверенность, Доверенность_номер, '')
  if(Задана_Доверенность, Получатель + ' ' + Доверенное_лицо_ФИО_дат_падеж, '')

  // Принял на ответственное хранение
  МОЛ_Должность_Получателя
  МОЛ_Получателя

  if (Тип_Операции = 4, Номер_документа, '')
  if (Тип_Операции = 4, DateToStr(Дата_документа, 'DD/MM/YYYY'), '')

  if (Тип_Операции <> 4, Номер_документа, '')
  if (Тип_Операции <> 4, DateToStr(Дата_документа, 'DD/MM/YYYY'), '')

  Главный_бухгалтер_Поставщика
  Главный_бухгалтер_Получателя
.endfields
