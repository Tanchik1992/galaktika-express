#CommissionLinkVar
#include funcOs.frn
end.  // funcOs.frn - вынес из файла чтобы не создавалась пустая строка перед формой
.{
.begin
  for (Хар1 := 1; Хар1 <= Count(Характеристика_Наименование_Объекта); Хар1 := Хар1 + 1)
  {
    Всего_наименов [Хар1] := '';
    Характеристика_Наименование_Объекта [Хар1] := '';
    Характеристика_Количество_Объектов  [Хар1] := '';
  }
  for (Хар2 := 1; Хар2 <= Count(Характеристика_NameDrag); Хар2 := Хар2 + 1)
  {
    Характеристика_NameDrag  [Хар2] := '';
    Характеристика_MassaDrag [Хар2] := '';
  }
  Хар1 := 0;
  Хар2 := 0;
  ХарN := 0;
end.
#CommissionLinkInit(osOne)
.begin
  sSeparatorList := '';
  if ((Информация_о_председателе <> '') and
      (Список_членов_комиссии <> ''))
  {
    if (not ReadMyDsk(sSeparatorList, 'SeparatorList', true))
      sSeparatorList := ',';
  }
end.
.fields
  '(Метод учета: ' +Метод_Учета +')'
  Поставщик
  Поставщик_ОКПО
  Структурное_подразделение_Поставщика
  Руководитель_Получателя_Должность
  Руководитель_Получателя
  Номер_документа
  if (Longint( Дата_документа)<>0, DateToStr( Дата_документа, 'DD/MM/YYYY'), '')
  if (Longint( Дата_приказа)<>0, DateToStr(Дата_приказа, 'DD'), '')
  if (Longint( Дата_приказа)<>0, DateToStr(Дата_приказа, 'MM'), '')
  if (Longint( Дата_приказа)<>0, DateToStr(Дата_приказа, 'YYYY'), '')
  Номер_приказа
  Информация_о_председателе + sSeparatorList
  Список_членов_комиссии
  НаименованиеОС
  ИнвНомер
  Заводской
  СчетДеб
  СубсчетДеб
.endfields
