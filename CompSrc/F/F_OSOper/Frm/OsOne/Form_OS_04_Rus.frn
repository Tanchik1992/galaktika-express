.begin
  if (Хар2 = 0)
    Хар2 := 1;

  ХарN := Хар1 + Хар2 - 1;
  Хар1 := 0;
end.
.{WHILE Хар1<ХарN
.begin
  Хар1 := Хар1 + 1;
end.
.fields
  if(Хар1 > Хар2, Характеристика_Наименование_Объекта [Хар1 - Хар2 + 1],
                  if(Хар1 = 1, Характеристика_Наименование_Объекта [1], ''))
  if(Хар1 > Хар2, Характеристика_Количество_Объектов  [Хар1 - Хар2 + 1],
                  if(Хар1 = 1, Характеристика_Количество_Объектов  [1], ''))

  Характеристика_NameDrag  [Хар1]
  Характеристика_N_Edin    [Хар1]
  Характеристика_MassaDrag [Хар1]
.endfields
