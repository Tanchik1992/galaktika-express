.p 86
.var
  Характеристика_Наименование_Объекта : array[1..10] of string;
  Характеристика_Количество_Объектов  : array[1..10] of string;
  Характеристика_NameDrag             : array[1..10] of string;
  Характеристика_N_Edin               : array[1..10] of string;
  Характеристика_MassaDrag            : array[1..10] of string;

  Хар1 : word; // Текущий номер в цикле состава
  Хар2 : word; // Текущий номер в цикле драгметаллов
  ХарN : word; // Количество элементов в массиве данных характеристики

  bNeedPrintEmptyCells : boolean; // надо ли печатать пустые строчки по услугам
  dCommonUslSumma      : double;  // общая сумма услуг
  тмп_СчетДеб          : string;
  тмп_СубсчетДеб       : string;
  тмп_СчетКре          : string;
  тмп_СубсчетКре       : string;
  bFoundOborot         : boolean;
.endvar
#CommissionLinkVar
#include funcOs.frn
end.  // funcOs.frn - вынес из файла чтобы не создавалась пустая строка перед формой
.{
.begin
  for (Хар1 := 1; Хар1 <= Count(Характеристика_Наименование_Объекта); Хар1 := Хар1 + 1)
  {
    Характеристика_Наименование_Объекта [Хар1] := '';
    Характеристика_Количество_Объектов  [Хар1] := '';
  }
  for (Хар2 := 1; Хар2 <= Count(Характеристика_NameDrag); Хар2 := Хар2 + 1)
  {
    Характеристика_NameDrag  [Хар2] := '';
    Характеристика_N_Edin    [Хар2] := '';
    Характеристика_MassaDrag [Хар2] := '';
  }
  Хар1 := 0;
  Хар2 := 0;
  ХарN := 0;
end.
#CommissionLinkInit(osOne)
.fields
  '(Метод учета: ' +Метод_Учета +')'
  Поставщик
