  if (LongInt(Дата_выбытия)<>0, DateToStr(Дата_выбытия, 'DD/MM/YYYY'), '')
  Номер_приказа
  Основание_для_составления_акта
  if (Longint(Дата_приказа)<>0, DateToStr(Дата_приказа, 'DD/MM/YYYY'), '')
  МОЛ_Поставщика
  Табельный_номер_Поставщика
  Руководитель_Поставщика_Должность
  Руководитель_Поставщика
  Номер_документа
  if (LongInt(Дата_документа)<>0, DateToStr(Дата_документа, 'DD/MM/YYYY'), '')
  Причина_списания
.endfields
