.linkform 'Move_MBP_012' prototype is Move_MBP
.Group 'Внутреннее перемещение'
.Group 'Ввод в эксплуатацию'
.NameInList 'Накладная-требование на отпуск материалов. Форма № M-11'
.f "LMoveMBP.OUT"
.Defo Landscape
.p 54
.var
  Count : Integer
  TotSum : Double
  SumIzn : Double
  KolDoc : Boolean
.endVar
.begin
  KolDoc := False
end.
.fields
 CommonFormHeader
 NameOrg
 Nom
 DateToStr(Dat,'DD/MM/YYYY')
 if (ToMOL = '', ToPodr, ToMOL)
 if (FromMOL = '', FromPodr, FromMOL)

 MbpName  MbpNum  MbpEd  Double(0)  otpKol  Price  SumPr  InvNum  PasNum  SklNum

 Sum  DoubleToStr( Count, '\m4')  DoubleToStr( Sum,'\2p\m4')

 if ( Pos( 'ноль', DoubleToStr( Sum, '\2p5')) <> 0, '=',
   DoubleToStr( Sum, '\2p88'))

 '/' + FrMolShort + '/'
 '/' + ToMolShort + '/'
.endfields
#include MBP_M11P.frn
.endform

