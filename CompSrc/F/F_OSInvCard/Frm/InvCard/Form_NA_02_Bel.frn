  Организация_ОКПО
  Подразделение
  МОЛ                                                                    // Ответственное лицо
  МОЛ_Табельный_номер                                                    // Табельный номер
  Дата_составления                                                       // Дата составления
                                                                         // Вид_Операции
  if (Проведенное_Поступление, 'Акт приемки нематериальных активов', '') // наименование
  if (Проведенное_Поступление, DateToStr(Дата_Документа_Получения  , 'DD/MM/YYYY'), '') // дата  Дата_Операции_Получения - из операции
  if (Проведенное_Поступление, Номер_Документа_Получения           , '') // номер
  Наименование_ОС
.endfields
