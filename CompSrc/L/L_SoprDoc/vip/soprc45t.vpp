Table STRUCT MTabOtchet "Таблица в памяти"
  (
    NRec          : comp,
    npp           : word,
    TDoc          : word,
    TDocStr       : string,
    TDocnRec      : comp,
    Descr         : string,
    dSopr         : Date,
    nSopr         : string,
    KatOrgName    : string,
    summa         : double,
    valut         : string,
    Skl           : string,
    Mol           : string
 )
With Index
  (
    TMP00indexmem = NRec(Unique, Surrogate, Journal)
   , TMP01indexmem = TDoc+npp
   , TMP02indexmem = TDoc+dSopr+nSopr
   , TMP03indexmem = TDoc+dSopr+nSopr+Skl+Mol
  );


