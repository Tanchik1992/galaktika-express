Table STRUCT MTabReestrforKN "Таблица в памяти"
  (
    NRec          : comp,
    Pick          : word,
    KatSoprnRec   : comp,
    KatSoprVidSopr: word,
    SpSoprnRec    : comp,
    KatSoprnSopr  : string,
    KatSoprdSopr  : Date,
    KatSoprdOpr   : Date,
    KatSoprcVal   : comp,
    KatSoprVhodNal: word,
    KatSoprSkid   : double,
    KatMCname     : string,
    KatPodrName   : string, //склад
    KatMolName    : string, //мол
    cKatPodr      : comp, //склад
    cKatMol       : comp, //мол
    KatPartyname  : string, //партия
    KatOrgname    : string,
    OldPrice      : double,
    NewPrice      : double,
    KolOpl        : double,
    PodrFrom      : string,
    MolFrom       : string
  )
With Index
  (
     TMP00indexmem = NRec(Unique, Surrogate, Journal)
    , TMP01indexmem = KatSoprnSopr
    , TMP02indexmem = KatSoprdSopr
    , TMP03indexmem = KatMCName
    , TMP04indexmem = KatPartyName+KatPodrName+KatMolName
    , TMP05indexmem = KatOrgName
    , TMP06indexmem = oldPrice
    , TMP07indexmem = NewPrice
    , TMP08indexmem = Pick+KatSoprnSopr+KatPodrName+KatMolName
    , TMP09indexmem = KatSoprdOpr
//    , TMP10indexmem = KatPodrName+KatMolName+KatPartyName
//    , TMP11indexmem = KatMolName+KatPodrName+KatPartyName
  );


