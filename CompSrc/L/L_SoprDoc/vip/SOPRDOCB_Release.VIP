HandleEvent
#include soprdb10.vpp

//-------------для Нал.Уч.---------------------------
cmPositionChanged:
  if (CurTable = #SpSopUsl) AND
      (boGetTune('Oper.NalUch'))
    set sumNalUch := SpSopUsl.priceN*SpSopUsl.kolFact;

