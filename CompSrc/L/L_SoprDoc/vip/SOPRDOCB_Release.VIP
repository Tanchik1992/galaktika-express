// Количество использованных в накладных
Function GetKolNaklUsed : double;
var dKol : double;
{
  GetKolNaklUsed := 0;

  dKol := 0;

  _LOOP SpecZatr2 where ((SpSopr.NRec == SpecZatr2.cAddSumTune))
    {
      if (SpecZatr2.coTable = 0)
        dKol := dKol + double(SpecZatr2.cSpec);
    }

  GetKolNaklUsed := dKol;
}

