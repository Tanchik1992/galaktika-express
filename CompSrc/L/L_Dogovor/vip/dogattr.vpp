
cmPickClass:
  {
    if (isNew)
      if (not isModified)
        {
          message(''#3'Переход невозможен, так как' +
                  ''#13#3'не заполнен документ.',okButton);
          Exit;
        }
    case curtable of
      #Dogovor1,
      #Dogovor: {
        if (UpDateInsertDogovor <> tsOk) {}

        if (Dogovor.nRec <> 0)
         {
           SaveMyDsk(NOT EditDogPosEnabled(false, false),
                     '_CanEditClassifier_');
           runInterface(ExClassifier, word(CurTableCode), Dogovor.Nrec);
         }
      }

      #SpDocs: {
        if ((SpDocs.cGrMCUSL = 0) and (SpDocs.cMCUSL = 0))
          {
            message(''#3'Укажите для позиции наименование МЦ/услуги либо удалите запись!',
                         Warning + OkButton);
            abort;
            exit;
          }
        oSpDocs.UpDate_current_SpDocs(SpDocs.BufferP);
        if (SpDocs.nRec <> 0)
         {
           SaveMyDsk(NOT EditDogPosEnabled(false, false),
                     '_CanEditClassifier_');
           runInterface(ExClassifier, word(CurTableCode), SpDocs.Nrec);
         }
      }
    end;
  }

