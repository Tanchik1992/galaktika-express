    {
      SaveMyDsk(word(56),'MainKolProt_Cen');
      MainKol:=30
    }
 if not readmydsk(FixKol,'FixKolProt_Cen',false)
    {
      SaveMyDsk(word(76),'FixKolProt_Cen');
