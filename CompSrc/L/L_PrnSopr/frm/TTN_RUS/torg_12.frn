  KolItog, MassaBrItog, MassaNetItog, Sum1Itog, Sum2Itog,Sum3Itog :Double;
  KolItogFull, MassaBrItogFull, MassaNetItogFull, Sum1ItogFull, Sum2ItogFull,Sum3ItogFull :Double;
  СтНДСНац : double;
  СтНацБН  : double;
  СтНацСН  : double;
  СпСимвол, strEmpty,
  KolGrM, KolOneGrM: string;
  bAutoGrM: boolean;
.endvar
#include org_decl.frn
#include format.frn
#include ttn_fun.frn
#include mfo.frn
.fields
!  CommonFormHeader
  #OrgFields(Грузоотправитель)
  Грузоотправитель_ОКПО
  #OrgFieldsBank(Грузоотправитель)
#ifdef _TTN_PRIH_
  Склад_Пол
  Получатель_ОКОНХ
#else
  Склад
  Поставщик_ОКОНХ               // KatOrg.OKONH
#end

  #OrgFields(Грузополучатель)
  Грузополучатель_ОКПО
  #OrgFieldsBank(Грузополучатель)

  if(Грузоотправитель = Поставщик,
     #OrgFieldsUr(Поставщик),
     #OrgFields  (Поставщик)
    )
  Поставщик_ОКПО
  #OrgFieldsBank(Поставщик)

#ifdef _TTN_PRIH_
  #OrgFields(Получатель)
  Получатель_ОКПО
  #OrgFieldsBank(Получатель)
#else
  if(Грузополучатель = Плательщик,
     #OrgFieldsUr(Плательщик),
     #OrgFields  (Плательщик)
    )
  Плательщик_ОКПО
  #OrgFieldsBank(Плательщик)
#end

  if (Номер_Договора <> '', 'Договор',
    if (НарядЗаказНомер <> '', 'Заказ-наряд',
      if (Основание <> '', 'Счет', '')))

  if (Номер_Договора <> '', Номер_Договора,
    if (НарядЗаказНомер <> '', НарядЗаказНомер,
      if (Основание <> '', Основание, '')))

  if (Номер_Договора <> '', DateToStr(Дата_Договора, 'DD/MM/YYYY'),
    if (НарядЗаказНомер <> '', DateToStr(НарядЗаказДата, 'DD/MM/YYYY'),
      if (Основание <> '', DateToStr(ДатаОснования, 'DD/MM/YYYY'), '')))

  номер
  if (Дата=date(0,0,0),'',DateToStr(Дата,'DD/MM/YYYY'))

!.{
  Nom
  Название КодМЦ
  УчЕд     CodeEd
  if (ЕД_ИЗМЕРЕНИЯ_ГРУЗОВЫХ_МЕСТ <> '', ЕД_ИЗМЕРЕНИЯ_ГРУЗОВЫХ_МЕСТ, ОтпЕд)
  KolOneGrM
  KolGrM
  if (МассаМЦ + МассаТарыМЦ = 0 , '', DoubleToStr((МассаМЦ + МассаТарыМЦ) * КолУч, Massa_Kg))
  if (wFillCol10 = 0,
    if (МассаМЦ = 0, '', DoubleToStr(МассаМЦ * КолУч, Massa_Kg)),
      DoubleToStr(КолУч, Kol_Pr)) // выводится по настройке
  DoubleToStr(ЦНацБНУч, Cena_PrR)
  DoubleToStr(СтНацБН,  Cena_PrR)
  DoubleToStr(ставка_первого_налога, NDS_St)
  DoubleToStr(СтНДСНац, Cena_PrR)
  DoubleToStr(СтНацСН,  Cena_PrR)
!.}
!.[F
  DoubleToStr(KolItog, RoundGrm)
  if (MassaBrItog = 0, '', DoubleToStr(MassaBrItog, Massa_Kg))
  if (wFillCol10 = 0,
    if (MassaNetItog = 0, '', DoubleToStr(MassaNetItog, Massa_Kg)), '')
  DoubleToStr(Sum1Itog, Cena_PrR)
  DoubleToStr(Sum2Itog, Cena_PrR)
  DoubleToStr(Sum3Itog, Cena_PrR)
  СпСимвол
!.]F
!.[F
  DoubleToStr(KolItog, RoundGrm)
  if (MassaBrItog = 0, '', DoubleToStr(MassaBrItog, Massa_Kg))
  if (wFillCol10 = 0,
    if (MassaNetItog = 0, '', DoubleToStr(MassaNetItog, Massa_Kg)), '')
  DoubleToStr(Sum1Itog, Cena_PrR)
  DoubleToStr(Sum2Itog, Cena_PrR)
  DoubleToStr(Sum3Itog, Cena_PrR)
  СпСимвол
!.]F
  DoubleToStr(KolItogFull, RoundGrm)
  if (MassaBrItogFull = 0 , '', DoubleToStr(MassaBrItogFull, Massa_Kg))
  if (wFillCol10 = 0,
    if (MassaNetItogFull = 0, '', DoubleToStr(MassaNetItogFull, Massa_Kg)), '')
  DoubleToStr(Sum1ItogFull, Cena_PrR)
  DoubleToStr(Sum2ItogFull, Cena_PrR)
  DoubleToStr(Sum3ItogFull, Cena_PrR)

  DoubleToStr(NomFull, '4\m')

  if (MassaNetItogFull = 0 , '',
    MassaInWords (MassaNetItogFull, if (wRoundMass2 > 3, wRoundMass2, 3))) // минимальная точность - 1кг.
  DoubleToStr(Round(KolItogFull, KolZn), strDr)

  if (MassaBrItogFull = 0, '',
    MassaInWords (MassaBrItogFull,  if (wRoundMass2 > 3, wRoundMass2, 3)))

  if(НомерДовер <> '', НомерДовер, '__________')
  if(LongInt(ДатаДовер) <> LongInt(0), DateToStr(ДатаДовер, 'DD mon YYYY'), '__________________')

  if ((Trim(ФИОДовер) <> '') AND (ДатаДовер <> Date(0,0,0) OR Trim(НомерДовер) <> ''),
    if (Trim(ОргДовер) <> '', ОргДовер, Грузополучатель), '')

  strokaR

 #ifdef _TTN_PRIH_
   strEmpty strEmpty
 #else
  if(отпуск_разрешил_должность <> '', отпуск_разрешил_должность, '___________________')
  if(отпуск_разрешил <> '', отпуск_разрешил, '________________________________')
 #end

  ДовЛицоДолжность + if (ДовЛицоДолжность <> '', ' ', '')
  + ФИОДовер

 #ifdef _TTN_PRIH_
   if(отпуск_разрешил_должность <> '', отпуск_разрешил_должность, '____________________')
   if(отпуск_разрешил <> '', отпуск_разрешил, '________________________________')
   strEmpty strEmpty strEmpty
   if(сдал_отправитель_должность <> '', сдал_отправитель_должность, '____________________')
   if(сдал_отправитель <> '', сдал_отправитель, '________________________________')
 #else
   strEmpty strEmpty
   Главный_бухгалтер
   if(сдал_отправитель_должность <> '', сдал_отправитель_должность, '_________________')
   if(сдал_отправитель <> '', сдал_отправитель, '________________________________')
   strEmpty strEmpty
 #end
  СпСимвол

.endfields
