  KolGrM, KolOneGrM: string;
  Kredit: string;
  TotKol : double;
  TotSum : double;
.endvar
.fields
  Номер

  Получатель
    + ' ИНН:'      + ПолучательУНН
    + ' Адрес:'    + Получатель_Адрес
    + ' Банк:'     + Получатель_БАНК_Имя
    + ' р/с '      + Получатель_БАНК_Счет
    + ' БИК:'      + Получатель_БАНК_Мфо
    + ' тел.: '    + Получатель_ТЕЛ
  Получатель_ОКПО

  if (Дата = Date(0,0,0), '', DateToStr(Дата, 'DD/MM/YYYY'))
  Номер
  Номер
  if (Дата = Date(0,0,0), '', DateToStr(Дата, 'DD/MM/YYYY'))

  Грузоотправитель + ' Адрес: ' + Грузоотправитель_Адрес + ' Тел: ' + Грузоотправитель_ТЕЛ
  Поставщик        + ' Адрес: ' + Поставщик_Адрес        + ' Тел: ' + Поставщик_ТЕЛ
  Грузополучатель  + ' Адрес: ' + Грузополучатель_Адрес  + ' Тел: ' + Грузополучатель_ТЕЛ

  Основание
  if (ДатаОснования = Date(0,0,0), '', DateToStr(ДатаОснования, 'DD/MM/YYYY'))

     KolGrM
     if (ЕД_ИЗМЕРЕНИЯ_ГРУЗОВЫХ_МЕСТ <> '', ЕД_ИЗМЕРЕНИЯ_ГРУЗОВЫХ_МЕСТ, ОтпЕд)
     Название
     УчЕд
     DoubleToStr(КолВес/1000, Massa_Tn)

  СпецСимвол

.endfields
.{
.begin
  TotKol := 0;
  TotSum := 0;
end.
.if tovn01
.end
.if tovn02
.end

