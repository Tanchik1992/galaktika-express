  НомерПоСмете_работы
  Название_работы
  Код_работы
  ЕдИзм_работы
  DoubleToStr(Кол_Работы, Kol_Pr)
  DoubleToStr(Сумма1_Работы / Кол_Работы, Cena_PrR)
  DoubleToStr(Сумма_Зарпл / Кол_Работы, Cena_PrR)
  DoubleToStr((Сумма_Зарпл_мех + Сумма_Экспл) / Кол_Работы, Cena_PrR)
  DoubleToStr(Сумма_Зарпл_мех / Кол_Работы, Cena_PrR)
  If (Сумма1_Работы = 0, '', DoubleToStr(Сумма1_Работы, Cena_PrR))
  If (Сумма_Зарпл = 0, '', DoubleToStr(Сумма_Зарпл, Cena_PrR))
  If (Сумма_Зарпл_мех + Сумма_Экспл = 0, '', DoubleToStr(Сумма_Зарпл_мех + Сумма_Экспл, Cena_PrR))
  If (Сумма_Зарпл_мех = 0, '', DoubleToStr(Сумма_Зарпл_мех, Cena_PrR))
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  //CHECKENTER KSKoeffPart1
  Название_Коэф
  Доп_Инфо_Коэф
  If (Сумма_Коэф = 0, '', DoubleToStr(Сумма_Коэф, Cena_PrR))
  If (Коэф_Зарпл = 0, '', DoubleToStr(Коэф_Зарпл, Cena_PrR))
  If (Коэф_Зарпл_мех + Коэф_Экспл = 0, '', DoubleToStr(Коэф_Зарпл_мех + Коэф_Экспл, Cena_PrR))
  If (Коэф_Зарпл_мех = 0, '', DoubleToStr(Коэф_Зарпл_мех, Cena_PrR))
  Название_Коэф
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  //CHECKENTER KSKoeffPart2
  Название_Коэф_Элем
  Доп_Инфо_Коэф_Элем
  If (Сумма_Коэф_Элем = 0, '', DoubleToStr(Сумма_Коэф_Элем, Cena_PrR))
