  If (Коэф_Зарпл_мех_Элем + Коэф_Экспл_Элем = 0, '', DoubleToStr(Коэф_Зарпл_мех_Элем + Коэф_Экспл_Элем, Cena_PrR))
  If (Коэф_Зарпл_мех_Элем = 0, '', DoubleToStr(Коэф_Зарпл_мех_Элем, Cena_PrR))
  Название_Коэф_Элем
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  //CHECKENTER KSKoeffPart3
  Название_Накл
  '(поз.' + Доп_Инфо_Накл + ')'
  DoubleToStr(Процент_Накл, '666.88') + '%'
  If (Сумма_Накл = 0, '', DoubleToStr(Сумма_Накл, Cena_PrR))
  If (Накл_Зарпл = 0, '', DoubleToStr(Накл_Зарпл, Cena_PrR))
  If (Накл_Зарпл_мех + Накл_Экспл = 0, '', DoubleToStr(Накл_Зарпл_мех + Накл_Экспл, Cena_PrR))
  If (Накл_Зарпл_мех = 0, '', DoubleToStr(Накл_Зарпл_мех, Cena_PrR))
     Название_Накл
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  //CHECKENTER KSKoeffPart4
  Название_Приб
  '(поз.' + Доп_Инфо_Приб + ')'
  DoubleToStr(Процент_Приб, '666.88') + '%'
  If (Сумма_Приб = 0, '', DoubleToStr(Сумма_Приб, Cena_PrR))
  If (Приб_Зарпл = 0, '', DoubleToStr(Приб_Зарпл, Cena_PrR))
  If (Приб_Зарпл_мех + Приб_Экспл = 0, '', DoubleToStr(Приб_Зарпл_мех + Приб_Экспл, Cena_PrR))
