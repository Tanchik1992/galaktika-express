  Название_Приб
  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  DoubleToStr(ПрямыеЗатраты11, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты12, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты13, Cena_PrR)
  DoubleToStr(ПрямыеЗатраты14, Cena_PrR)

  DoubleToStr(ПрямыеЗатратыИтого11, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого12, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого13, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого14, Cena_PrR)

  //CHECKENTER KSKoeffSmet
  Название_КоэфСмет
  ' (' + DoubleToStr(ПроцентКоэф, '666.88') + '%)'
  if (Сумма_КоэфСмет = 0, '', DoubleToStr(Сумма_КоэфСмет, Cena_PrR))
  if (КоэфСмет_Зарпл = 0, '', DoubleToStr(КоэфСмет_Зарпл, Cena_PrR))
  if (КоэфСмет_Зарпл_мех + КоэфСмет_Экспл = 0, '', DoubleToStr(КоэфСмет_Зарпл_мех + КоэфСмет_Экспл, Cena_PrR))
  if (КоэфСмет_Зарпл_мех = 0, '', DoubleToStr(КоэфСмет_Зарпл_мех, Cena_PrR))

  DoubleToStr(ПрямыеЗатратыИтого11, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого12, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого13, Cena_PrR)
  DoubleToStr(ПрямыеЗатратыИтого14, Cena_PrR)

  ФИО
