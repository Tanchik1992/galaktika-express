      VDrag.GetFirst NalDrag where ((NalDragPar.nrec == NalDrag.nrec));

    VDrag.TmpSaldo2.kol := VDrag.TmpSaldo2.kol  + begkol * VDrag.NalDrag.Kol;
    VDrag.TmpSaldo2.kolP:= VDrag.TmpSaldo2.kolP + addkol * VDrag.NalDrag.Kol;
