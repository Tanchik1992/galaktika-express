#doc
Количественный учет
Сличительная ведомость результатов инвентаризации ТМЦ N 19
#end
.linkform 'SLIC_RZ_n19_1' prototype is 'SLIC_RZ'
.nameInList 'Сличительная ведомость результатов инвентаризации ТМЦ N 19'
.Group 'Количественный учет'
.var
  i, ipt, pkol, pkol2 : integer;
  pkoli, pkoln        : double;
  psumi, psumn        : double;
.endvar
.create view slic_log1
  as select
    KatOrg.Name, KatOrg.OKPO
  from
    KatOrg              (ReadOnly)
#ifndef GAL5_80
    , SystDate            (ReadOnly)
#end
  where
   ((
#ifndef GAL5_80
    SystDate.nRecMyOrg == KatOrg.nRec
#else
    coGetTune('MyOrg') == KatOrg.nRec
#end
   ))
;
.fields
    CommonFormHeader
    if (SLIC_LOG1.GetFirst = 0, SLIC_LOG1.KatOrg.Name, '')
    if (SLIC_LOG1.GetFirst = 0, SLIC_LOG1.KatOrg.OKPO, '')
    склад
    дата_инвентаризации
    'true' : 's'
.endfields
.begin
  i    := 0;
  pkol := 0;
end.
