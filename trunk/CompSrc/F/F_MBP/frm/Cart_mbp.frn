.DEFP PA4
.PAPER PA4
.P 38
.ORIENT PORTRAIT
.LL 80
.Fields
 CommonFormHeader
 InNumDoc
 NameOrg
 ОКПО
 DateToStr(InDate,'DD/MM/YYYY')
 InPodrFrom
 InPodr
 InMOLFrom
 if ( Trim( InPers) <> '', InPers, InMol)
 InMOLFrom

 MbpName  MbpNum  MbpEd  Double(0)  InKolDoc  InPrice
 if ( wGetTune( 'Fin.MBP.OstStoim') = 0, InSumDoc, InSumDoc- (InSumIsnPer+ InSumIsnNach))
 SklNum

 InMolFrom  if ( Trim( InPers) <> '', InPers, InMolShort)
.endFields
