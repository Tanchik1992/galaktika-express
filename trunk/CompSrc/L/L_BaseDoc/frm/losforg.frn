! логическая таблица для получения полного адреса организации
.create view loFullOrgAdr
var cRecOrg : comp;
from
  KatOrg,
  KatCity,
  KatState,
  PodrInfo,
  AddressN
where
((
     cRecOrg           == KatOrg.nRec
and  KatOrg.cCity      == KatCity.nRec
and  KatOrg.cState     == KatState.nRec
));
