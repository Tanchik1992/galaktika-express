Procedure DeleteBaseDoc(pRec: comp; pTipDO: word);
{
  TipDO := pTipDO;
  if ( GetFirst FastFirstRow BaseDoc where (( pRec == BaseDoc.NRec )) = tsOk )
    DoBaseDocDelete(false);
}

