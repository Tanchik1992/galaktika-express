.fields
! Header на лист
 CommonFormHeader
  'По '+AllPartyTxt+' '+AllPartyVTxt+' По '+AllMcUslTxt+' '+AllMcUslVTxt+
            if(pEdName='','',' Уч.ед.: '+pEdName)
  DateToStr(d2,'DD/MM/YYYY')
  DateToStr(d3,'DD/MM/YYYY')
  'по '+if(ByOpr=0
          , 'всем (по выписке)'
          , if(ByOpr=2
              ,'выполненным'
              ,'выписанным'
              )
          )+ if(NaklAkt=1
               ,' накладным'
               , if(NaklAkt=2
                   ,' актам'
                   ,' накладным, актам'
                   )
               ) +
          if(Longint(d1)=0,'',' с '+DateToStr(d1,'DD/MM/YYYY'))+
          if(longint(d4)=0,'',' по '+DateToStr(d4,'DD/MM/YYYY'))
  'Отгружено (выполнено)'+
    if (TysEd=2,', в миллионах учетных единиц'
       , if (TysEd=1,', в тысячах учетных единиц',''))+
     if (RubTysMln=1,', в тысячах рублей',
       if (RubTysMln=2,', в миллионах рублей',''))
  'Оплачено'+
    if (TysEd=2,', в миллионах учетных единиц'
       , if (TysEd=1,', в тысячах учетных единиц',''))+
     if (RubTysMln=1,', в тысячах рублей',
       if (RubTysMln=2,', в миллионах рублей',''))
  if(TysEd=2,'(в млн)',if(TysEd=1,'(в тыс)',''))
  'Сумма ('+
  if (RubTysMln=1,'тыс.',
    if (RubTysMln=2,'млн.',''))+'руб.)'
  PrNds1
  PrNds2
  PrNds1
  PrNds2
  'Сумма ('+
  if (RubTysMln=1,'тыс.',
    if (RubTysMln=2,'млн.',''))+'руб.)'

! отчет
  CommonFormHeader
  'по '+if(ByOpr=0
          , 'всем (по выписке)'
          , if(ByOpr=2
              ,'выполненным'
              ,'выписанным'
              )
          )+ if(NaklAkt=1
               ,' накладным'
               , if(NaklAkt=2
                   ,' актам'
                   ,' накладным, актам'
                   )
               ) +
          if(Longint(d1)=0,'',' с '+DateToStr(d1,'DD/MM/YYYY'))+
          if(longint(d4)=0,'',' по '+DateToStr(d4,'DD/MM/YYYY'))
  DateToStr(d2,'DD/MM/YYYY')
  DateToStr(d3,'DD/MM/YYYY')
.endfields
