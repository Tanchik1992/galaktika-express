function NotEditPosStatus (msg : boolean) : boolean; // true - งฏเฅ้โ์
var 
  code : boolean;
{
  code := false;

  if (oStatUser.Stat_Nastr(KatSopr.VidSopr))
    {
      if (not oStatUser.Descr_Stat_D(false, KatSopr.VidSopr, sGetTune('User.Descr'), KatSopr.cNote))
        code := true;
    }
  else
    if ((boGetTune('Doc.StatusDoSopr')) and (KatSopr.Status <> 0))
      code := true;

  if (code and msg)
    Message(''#3'ฌ งฏเฅ้ฅญฎ เฅคชโจเฎขโ์ คฎชใฌฅญโ ' +
                'ข แโโใแฅ "' + KatNotes.Name + '"!', Information+OkButton);

  NotEditPosStatus := code;
}