case SpSopr.PrMC of
  2..4:
    {
      ProcRb := ProcRb + TempR;    ProcVb := ProcVb + TempV;
      ProcRn := ProcRn + TempRn;   ProcVn := ProcVn + TempVn;
      loStdPrb.nProcRn := loStdPrb.nProcRn + loStdPrb.posNDSR;
      loStdPrb.nProcVn := loStdPrb.nProcVn + loStdPrb.posNDSV;
    }
  5 :
    {
      SumVRb := SumVRb + TempR;    SumVVb := SumVVb + TempV;
      SumVRn := SumVRn + TempRn;   SumVVn := SumVVn + TempVn;
      loStdPrb.nSumVRn := loStdPrb.nSumVRn + loStdPrb.posNDSR;
      loStdPrb.nSumVVn := loStdPrb.nSumVVn + loStdPrb.posNDSV;
    }
  6 :
    {
      SumORb := SumORb + TempR;    SumOVb := SumOVb + TempV;
      SumORn := SumORn + TempRn;   SumOVn := SumOVn + TempVn;
      loStdPrb.nSumORn := loStdPrb.nSumORn + loStdPrb.posNDSR;
      loStdPrb.nSumOVn := loStdPrb.nSumOVn + loStdPrb.posNDSV;
    }
  7 :
    {
      SumKolRb := SumKolRb + TempR;    SumKolVb := SumKolVb + TempV;
      SumKolRn := SumKolRn + TempRn;   SumKolVn := SumKolVn + TempVn;
      loStdPrb.nSumKolRn := loStdPrb.nSumKolRn + loStdPrb.posNDSR;
      loStdPrb.nSumKolVn := loStdPrb.nSumKolVn + loStdPrb.posNDSV;
    }
  8 :
    {
      Sum_KP_Rb := Sum_KP_Rb + TempR;    Sum_KP_Vb := Sum_KP_Vb + TempV;
      Sum_KP_Rn := Sum_KP_Rn + TempRn;   Sum_KP_Vn := Sum_KP_Vn + TempVn;
      loStdPrb.nSum_KP_Rn := loStdPrb.nSum_KP_Rn + loStdPrb.posNDSR;
      loStdPrb.nSum_KP_Vn := loStdPrb.nSum_KP_Vn + loStdPrb.posNDSV;
    }
end;
