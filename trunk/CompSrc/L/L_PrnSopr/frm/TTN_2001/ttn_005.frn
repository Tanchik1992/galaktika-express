.begin
  var lnSName, lnSCode, KolLine, LengthPrice: word
  var sName, sName2, sCode: string
  var piExClass  : iExtClass;
  var _ClassCode : word;
  var KolTMP,i,Options,MainKol: word;
  var isKolSave : boolean
  var ii:double;
  var KolInSpSopr : integer;

#ifdef _TTN_FRN_
 #ifdef _TTN_2001F_
   lnSName     := 24;

  #ifdef _TTN_2001R_
   lnSName   := 19;
  #end

  #ifdef _TTN_2001P_
   lnSCode     := 7;
  #else
   lnSCode     := 10;
  #end

  #ifdef _TTN_2001R_
   lnSCode   := 8;
  #end

   LengthPrice := 27;
   readmydsk(MainKol,'MainKolTTN2001',false)
   readmydsk(FixKol,'FixKolTTN2001',false)
   readmydsk(Options,'OptionsTTN2001',false)
 #else
   lnSName     := 27;
   lnSCode     := 11;
   LengthPrice := 39;
   readmydsk(MainKol,'MainKolTTN',false)
   readmydsk(FixKol,'FixKolTTN',false)
   readmydsk(Options,'OptionsTTN',false)
 #end
#end

#ifdef _TN2_PC_FRN_
 #ifdef _TTN_2001F_
  lnSName    := 21;
  lnSCode    := 12;
 #else
  lnSName    := 20;
  lnSCode    := 11;
 #end
  LengthPrice := 32;
  readmydsk(MainKol,'MainKolTN2',false)
  readmydsk(FixKol,'FixKolTN2',false)
  readmydsk(Options,'OptionsTN2',false)
#end
  If Options=1 СпецСимвол:='';

!  GetVipInterface(piExClass);
  KolSpSopr.pNrec := накладная_нрек;

!выводится последней строкой в спецификации
  If ПрейскурантЦен <> ''
    {
     fixkol := fixkol - KolAddInStr (ПрейскурантЦен, LengthPrice, true);
    }

  KolTMP := 0;
  i      := 0; //номер страницы
  KolInSpSopr := количество_SpSopr;

  var isExistsClass: boolean;

  #ifdef _TTN_DOC_ROZN_
    PRN_DOPINFO.wTable := coRzSpDoc
  #else
    PRN_DOPINFO.wTable := coSpSopr
  #end
    isExistsClass := (PRN_DOPINFO.GetFirst ExClassName = tsOk)

  KolSpSopr._LOOP SpSopr
  {
    KolLine := 0;
    sName   := '';

    if (isExistsClass)
    {
     #ifdef _TTN_DOC_ROZN_
      PRN_DOPINFO.pNRec := KolSpSopr.SpSopr.cOwner;
     #else
      PRN_DOPINFO.pNRec := KolSpSopr.SpSopr.NRec;
     #end

      PRN_DOPINFO._Loop ExClassVal
        if (PRN_DOPINFO.GetFirst ExClassSeg = tsOk)
          sName := sName + '; ' + PRN_DOPINFO.ExClassSeg.Name;

      if (sName = '')
      {
        PRN_DOPINFO.pNRec := KolSpSopr.KatParty.NRec;

        PRN_DOPINFO._Loop ExClassVal
          if (PRN_DOPINFO.GetFirst ExClassSeg = tsOk)
            sName := sName + '; ' + PRN_DOPINFO.ExClassSeg.Name;
      }

      if (sName = '')
      {
        PRN_DOPINFO.pNRec := KolSpSopr.KatMC.NRec;

        PRN_DOPINFO._Loop ExClassVal
          if (PRN_DOPINFO.GetFirst ExClassSeg = tsOk)
            sName := sName + '; ' + PRN_DOPINFO.ExClassSeg.Name;
      }
    }

   #ifdef _TTN_ALC_
    _ClassCode := piExClass.GetClassCode (coSpSopr, 'СТРАНА ВВОЗА');
    sName2     := piExClass.GetSegName (_ClassCode, KolSpSopr.SpSopr.NRec);
    if (sName2 = '')
    {
      _ClassCode := piExClass.GetClassCode (coKatParty, 'СТРАНА ВВОЗА');
      sName2     := piExClass.GetSegName (_ClassCode  , KolSpSopr.KatParty.NRec);
    }
   #end

    if (isTTN_Fast)
    {
      MCDopInfo [KolSpSopr.SpSopr.Npp] := sName;
      MCStrana  [KolSpSopr.SpSopr.Npp] := sName2;
    }

    sName :=   KolSpSopr.KatMc.Name
             + if(sName  <> '', ' ('  + sName + ')', '')
             + if(sName2 <> '', ' ; ' + sName2     , '');

    sCode := if (bBarKodMC, KolSpSopr.KatMc.BarKod, '');

    KolTMP := KolTMP + 1;//на какой строке на листе нахожусь
    isKolSave := false;

    if (length(sName) > lnSName) or (length(sCode) > lnSCode)
    {
      KolLine     := KolAddInStr (sName, lnSName, false);// кол. доп. строк.
      var tKolLine: word;
      tKolLine    := KolAddInStr (sCode, lnSCode, false);

      if (tKolLine > KolLine)
        KolLine := tKolLine;

      KolInSpSopr := KolInSpSopr + KolLine;//общее к-во строк в спецификации

      KolTMP := KolTMP + KolLine;

      If (KolTMP > FixKol) //количество строк на i-ом листе
      {
           i := i + 1;

       #ifdef _TTN_FRN_
         #ifdef _TTN_2001F_
           SaveMyDsk(word(KolTMP - KolLine - 1), 'KolLineInTTN2001' + string(i));
       #else
           SaveMyDsk(word(KolTMP - KolLine - 1), 'KolLineInTTN'     + string(i));
         #end
       #end

       #ifdef _TN2_PC_FRN_
           SaveMyDsk(word(KolTMP - KolLine - 1), 'KolLineInTN2'     + string(i));
       #end
           KolTMP := KolLine + 1;//недопечатанное наим. МЦ
           isKolSave := true;
      }
    }//if

    if (KolTMP = FixKol) and (not isKolSave)  //количество строк на i-ом листе
    {
          i := i + 1;
      #ifdef _TTN_FRN_
        #ifdef _TTN_2001F_
          SaveMyDsk(word(FixKol), 'KolLineInTTN2001' + string(i));
        #else
          SaveMyDsk(word(FixKol), 'KolLineInTTN'     + string(i));
        #end
      #end

      #ifdef _TN2_PC_FRN_
          SaveMyDsk(word(FixKol), 'KolLineInTN2'     + string(i));
      #end
          KolTMP := 0;
    }
  }//._LOOP


  if (KolTmp > 0) //если есть недопечатанное название МЦ
  {
          i := i + 1;  //количество строк на последнем листе
      #ifdef _TTN_FRN_
        #ifdef _TTN_2001F_
          SaveMyDsk(word(FixKol), 'KolLineInTTN2001' + string(i));
        #else
          SaveMyDsk(word(FixKol), 'KolLineInTTN'     + string(i));
        #end
      #end

      #ifdef _TN2_PC_FRN_
          SaveMyDsk(word(FixKol), 'KolLineInTN2'     + string(i));
      #end
  }

#ifdef _TTN_FRN_
  if (KolInSpSopr > MainKol)
    CheckKol:=true;//количество строк на первом листе
  if (KolInSpSopr > MainKol - 1) and (ПрейскурантЦен <> '')
    CheckKol:=true; //чтобы печатало приложение
#end

#ifdef _TN2_PC_FRN_
  if (KolInSpSopr > MainKol)
    CheckKol:=true;//количество строк на первом листе
  if (KolInSpSopr > MainKol - 1) and (ПрейскурантЦен <> '')
    CheckKol:=true;//чтобы печатало приложение
#end

!  FreeVipInterface(piExClass);
end.
